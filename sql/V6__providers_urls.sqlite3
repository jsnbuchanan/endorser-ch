
CREATE TABLE give_provider (
  giveId TEXT, -- jwtId of the GiveAction which has this as a provider
  providerId TEXT, -- jwtId of the provider claim
  providerType TEXT -- eg. 'Person', 'Organization', 'GiveAction'
);
CREATE INDEX give_provider_give ON give_provider(giveId);
CREATE INDEX give_provider_provider ON give_provider(providerId);

